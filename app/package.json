{
  "name": "app",
  "homepage": "https://floydjd.github.io/",
  "version": "1.0.0",
  "private": "true",
  "workspaces": [
    "packages/*"
  ],
  "license": "MIT",
  "scripts": {
    "start": "yarn --cwd packages/client start",
    "build": "yarn --cwd packages/client build",
    "test": "yarn --cwd packages/client test",
    "eject": "yarn --cwd packages/client eject",
    "lint": "yarn --cwd packages/client lint",
    "hoist": "cp -R packages/client/build/. ..",
    "update": "yarn build && yarn hoist"
  },
  "devDependencies": {
    "@craco/craco": "^6.1.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^4.8.0",
    "@typescript-eslint/parser": "^4.8.0",
    "eslint": "^7.13.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "react-scripts": "4.0.3",
    "typescript": "4.2.3"
  }
}
